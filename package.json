{
  "name": "use-thing",
  "version": "1.0.0",
  "description": "A 2D/3D hooks library based on Vue3",
  "main": "index.js",
  "scripts": {
    "docs": "pnpm run build:source && pnpm run copyMd && vitepress dev packages",
    "docs:dev": "cross-env DEV=development vitepress dev packages",
    "build:rollup": "rollup -c",
    "build:source": "esno dataSource/readInData.ts",
    "copyMd": "pnpm run copy:utils",
    "copy:utils": "esno build/copyMarkdown.ts"
  },
  "keywords": [
    "vue",
    "vuething"
  ],
  "author": "upmin-l",
  "license": "MIT",
  "devDependencies": {
    "@iconify/json": "^2.1.73",
    "@rollup/plugin-json": "^4.1.0",
    "@tweenjs/tween.js": "^18.6.4",
    "@types/echarts": "^4.9.15",
    "@types/fs-extra": "^9.0.13",
    "@types/marked": "^4.0.3",
    "@types/node": "^18.0.0",
    "@vue/theme": "^1.0.2",
    "chalk": "^5.0.1",
    "cross-env": "^7.0.3",
    "echarts": "^5.3.3",
    "esbuild-register": "^3.3.3",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.1.0",
    "gray-matter": "^4.0.3",
    "marked": "^4.0.18",
    "rollup": "^2.75.7",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-esbuild": "^4.9.1",
    "typescript": "4.7.3",
    "unocss": "^0.42.0",
    "unplugin-icons": "^0.14.7",
    "unplugin-vue-components": "^0.21.0",
    "vite": "^2.9.13",
    "vite-plugin-inspect": "^0.6.0",
    "vitepress": "1.0.0-alpha.4",
    "vitepress-theme-demoblock": "^1.4.2",
    "vue": "^3.2.37"
  },
  "pnpm": {
    "overrides": {
      "vue": "^3.2.37"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,md}": [
      "eslint --fix"
    ]
  }
}
